

int Q1PNP = 1;
//int Q2NPN = 2;
//int Q3PNP = 3;
//int Q4NPN = 4;

int SWITCH1;



void setup() {
  // put your setup code here, to run once:
  pinMode(Q1PNP, OUTPUT);

  
//  pinMode(Q2NPN, OUTPUT);
//  pinMode(Q3PNP, OUTPUT);
//  pinMode(Q4NPN, OUTPUT);
}

void loop() {

  digitalWrite(Q1PNP, HIGH);
  delay(2000);
  digitalWrite(Q1PNP, LOW);
  delay(2000);
}


