void runMotorForward(){
  digitalWrite(Q1PNP, HIGH);
//  digitalWrite(Q4NPN, HIGH);
  delay(100);
  digitalWrite(Q1PNP, LOW);
//  digitalWrite(Q4NPN, LOW);
  
}


